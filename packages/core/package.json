{
  "name": "@zennnn/core",
  "version": "0.36.7",
  "private": true,
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "files": [
    "src/",
    "lib/",
    "types/"
  ],
  "scripts": {
    "build": "rm -rf ./lib/* && yarn build:lib && yarn build:bundle",
    "build:lib": "babel src --out-dir lib --source-maps",
    "build:bundle": "rollup -c",
    "generate:examples": "node ./scripts/generate-examples.js",
    "generate:api": "node ./scripts/generate-api.js",
    "test": "jest",
    "lint": "eslint src -f codeframe",
    "docs:dev": "vitepress dev docs",
    "docs:serve": "vitepress serve docs",
    "docs:build": "yarn generate:examples && yarn generate:api && vitepress build docs"
  },
  "dependencies": {
    "@zennnn/icons": "^0.36.7"
  },
  "devDependencies": {
    "vitepress": "^0.11.4"
  }
}
