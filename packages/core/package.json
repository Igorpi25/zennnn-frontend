{
  "name": "@zennnn/core",
  "version": "0.36.7",
  "private": true,
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "files": [
    "src/",
    "lib/",
    "types/"
  ],
  "scripts": {
    "build": "rm -rf ./lib/* && yarn build:lib && yarn build:bundle",
    "build:lib": "tsc -p tsconfig.dist.json",
    "build:bundle": "rollup -c",
    "gen:examples": "node ./scripts/generate-examples.js",
    "gen:api": "node ./scripts/generate-api.js",
    "test": "jest",
    "lint": "eslint src -f codeframe",
    "docs:dev": "vitepress dev docs --port 3030",
    "docs:serve": "vitepress serve docs --port 5050",
    "docs:build": "yarn gen:examples && yarn gen:api && vitepress build docs",
    "playground": "vite playground --port 4040",
    "playground:build": "vite build playground",
    "docs-dev": "node ./docs-ssr/server.js",
    "docs-build": "yarn docs-build:client && yarn docs-build:server",
    "docs-build:client": "vite build docs-ssr --ssrManifest --outDir dist/client",
    "docs-build:server": "vite build docs-ssr --ssr src/entry-server.js --outDir dist/server",
    "docs-generate": "vite build docs-ssr --ssrManifest --outDir dist/static && yarn docs-build:server && node ./docs-ssr/prerender",
    "docs-serve": "cross-env NODE_ENV=production node ./docs-ssr/server"
  },
  "dependencies": {
    "@zennnn/icons": "^0.36.7"
  },
  "devDependencies": {
    "vitepress": "^0.13.2"
  }
}
