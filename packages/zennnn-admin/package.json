{
  "name": "zennnn-admin",
  "version": "0.36.7",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint --mode production",
    "schema": "apollo-codegen introspect-schema http://localhost:9000/graphql --output schema.json",
    "schema-generate": "apollo-codegen generate src/graphql/**/*.tsx --schema schema.json --target typescript --output src/graphql/types.ts && prettier --write src/graphql/types.ts"
  },
  "dependencies": {
    "@zennnn/core": "^0.36.7",
    "@zennnn/icons": "^0.36.7",
    "core-js": "^3.14.0",
    "vue": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/eslint-parser": "^7.13.14",
    "@vue/cli-plugin-babel": "~5.0.0-beta.2",
    "@vue/cli-plugin-eslint": "~5.0.0-beta.2",
    "@vue/cli-plugin-typescript": "~5.0.0-beta.2",
    "@vue/cli-plugin-unit-jest": "~5.0.0-beta.2",
    "@vue/cli-service": "~5.0.0-beta.2",
    "@vue/compiler-sfc": "^3.1.1",
    "eslint": "^7.18.0",
    "eslint-plugin-vue": "^7.9.0"
  }
}
