{
  "name": "zennnn-admin",
  "version": "0.36.7",
  "private": true,
  "scripts": {
    "serve": "TAILWIND_MODE=watch vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:stable": "vue-cli-service build --mode stable",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint --mode production",
    "download-schema": "apollo client:download-schema --endpoint http://localhost:9000/graphql schema.json",
    "codegen": "apollo codegen:generate --includes=src/graphql/queries.ts,src/graphql/mutations.ts --localSchemaFile=schema.json --target=typescript --tagName=gql --no-addTypename --outputFlat src/graphql/types.ts"
  },
  "dependencies": {
    "@zennnn/core": "^0.36.7",
    "@zennnn/icons": "^0.36.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0-beta.2",
    "@vue/cli-plugin-eslint": "~5.0.0-beta.2",
    "@vue/cli-plugin-typescript": "~5.0.0-beta.2",
    "@vue/cli-plugin-unit-jest": "~5.0.0-beta.2",
    "@vue/cli-service": "~5.0.0-beta.2"
  }
}
